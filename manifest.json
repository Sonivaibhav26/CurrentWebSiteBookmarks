{
  "manifest_version": 2,
  "name": "Current WebSite Bookmarks",
  "description": "A Simple add on to view all the bookmarks from current webiste you are browsing",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "browserAction/index.html",
    "default_title": "Current WebSite Bookmarks"
  }
}